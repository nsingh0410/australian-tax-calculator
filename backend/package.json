{
  "name": "australian-tax-calculator-dual",
  "version": "1.0.0",
  "description": "Australian tax calculator with CLI and GraphQL API interfaces",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:cli": "node dist/index.js",
    "start:graphql": "node dist/graphql-server.js",
    "dev:cli": "ts-node src/index.ts",
    "dev:graphql": "ts-node src/graphql-server.ts",
